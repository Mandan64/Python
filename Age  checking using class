class Age(object):
    def __init__(self,age):
        self.age = age
    def ageCheck(self):
        if self.age in range(1,18):
            print("You are too yough to take part it")
        elif self.age in range(18,80):
            print("Ok you are selected")
        else:
            print("Go and check it out your birth certificate")
age = int(input("Enter your age: "))
display = Age(age)
display.ageCheck()
